plugin.tx_ximatypo3frontendedit {
  settings {
    # cat=xima_typo3_frontend_edit/settings/p; type=string; label=Ignore PIDs (and subpages) from frontend edit
    ignorePids =
    # cat=xima_typo3_frontend_edit/settings/p; type=string; label=Ignore content types from frontend edit
    ignoreCTypes =
    # cat=xima_typo3_frontend_edit/settings/p; type=string; label=Ignore plugin types from frontend edit
    ignoreListTypes =
    # cat=xima_typo3_frontend_edit/settings/p; type=string; label=Ignore specific uid of content elements from frontend edit
    ignoredUids =

    defaultMenuStructure {
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the info section
      div_info = 1
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the header
      header = 1
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the edit section
      div_edit = 1
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the edit content element button
      edit = 1
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the edit page button
      edit_page = 1
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the action section
      div_action = 1
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the hide button
      hide = 1
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the move button
      move = 1
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the info button
      info = 1
      # cat=xima_typo3_frontend_edit/defaultMenuStructure/p; type=boolean; label=Show the history button
      history = 1
    }
  }
}
